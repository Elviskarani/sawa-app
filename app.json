{
  "expo": {
    "name": "sawacharge",
    "slug": "sawacharge",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/applogo1.png",
    "scheme": "sawacharge",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.elvis236.sawacharge"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/applogo1.png",
        "backgroundImage": "./assets/images/applogo1.png",
        "monochromeImage": "./assets/images/applogo1.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.elvis236.sawacharge",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDfSacAaLtqsG_WpYA39jGbNezcCg7y2Vs"
        }
      }
    },
    "web": {
      "output": "static",
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/applogo1.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/LeagueSpartan-Black.ttf",
            "./assets/fonts/LeagueSpartan-Bold.ttf",
            "./assets/fonts/LeagueSpartan-ExtraBold.ttf",
            "./assets/fonts/LeagueSpartan-ExtraLight.ttf",
            "./assets/fonts/LeagueSpartan-Light.ttf",
            "./assets/fonts/LeagueSpartan-Medium.ttf",
            "./assets/fonts/LeagueSpartan-Regular.ttf",
            "./assets/fonts/LeagueSpartan-SemiBold.ttf",
            "./assets/fonts/LeagueSpartan-Thin.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "106ff5f1-8afe-4ac6-829e-2513e3419261"
      }
    }
  }
}
